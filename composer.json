{
	"name": "JYmusic/laravel-addons",
	"type": "framework-extension",
	"description": "Laravel Extension Pack",
	"license": "MIT",
	"keywords": ["Laravel", "Laravel 5", "addon", "migration", "architecture", "facade"],
	"homepage": "https://github.com/jymusic",
	"authors": [
		{
			"name": "JYmusic",
			"email": "zhangcb1984@163.com"
		}
	],
	"require": {
		"php": "^7.0",
        "laravel/framework": "~5.5|^6.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "~0.9"
    },
	"autoload": {
		"psr-4": {
			"JYmusic\\laravel-addons\\": "src/"
		},
		"files": [
			"src/Helpers.php"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"JYmusic\\LaravelAddons\\ServiceProvider"
			],
			"aliases": {
			}
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
