{
	"name": "hinet/laravel-addons",
	"type": "framework-extension",
	"description": "Addons Extension Pack for Laravel",
	"license": "MIT",
	"keywords": ["Laravel", "Laravel 5", "Laravel 6","Laravel 7","addon", "migration", "architecture", "facade"],
	"homepage": "https://github.com/hinet/laravel-addons",
	"authors": [
		{
			"name": "JYmusic",
			"email": "zhangcb1984@163.com"
		}
	],
	"require": {
		"php": "^7.2.5",
        	"laravel/framework": "~5.5|^6.0|^7.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1"
    },
	"autoload": {
		"psr-4": {
			"Hinet\\LaravelAddons\\": "src/"
		},
		"files": [
			"src/Helpers.php"
		]
	},
	"extra": {
		"laravel": {
			"providers": [
				"Hinet\\LaravelAddons\\ServiceProvider"
			],
			"aliases": {
			}
		}
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
